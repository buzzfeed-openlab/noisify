{
  "manifest_version": 2,
  "name": "Noisify",
  "description": "Add noise to your Facebook activity!",
  "author": "Cathy Deng",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "homepage_url": "https://www.buzzfeed.com/openlab",
  "icons": {
            "16": "icon16.png",
            "128": "icon128.png"
          },
  "web_accessible_resources": ["words/word_collection.txt"]
}
